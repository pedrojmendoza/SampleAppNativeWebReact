{
  "name": "my-native-web-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-native": "^0.52.0",
    "react-native-web": "^0.5.3",
    "react-scripts": "1.1.1"
  },
  "scripts": {
    "start-web": "react-scripts start",
    "build-web": "react-scripts build",
    "test-web": "react-scripts test --env=jsdom",
    "eject-web": "react-scripts eject",
    "start-native": "react-native-scripts start",
    "eject-native": "react-native-scripts eject",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test-native": "node node_modules/jest/bin/jest.js --watch",
    "test": "npm run test-web && npm run test-native",
    "start": "react-native start",
    "beta:android": "cd ./android && fastlane beta",
    "prod:android": "cd ./android && fastlane deploy"
  },
  "devDependencies": {
    "babel-plugin-react-native-web": "^0.5.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-preset-expo": "^4.0.0",
    "flow-bin": "^0.68.0",
    "react-test-renderer": "^16.2.0"
  },
  "jest": {}
}
